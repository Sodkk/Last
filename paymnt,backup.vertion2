<script>
    
    // Function to set cookie
    function setCookie(name, value, days) {
        var expires = "";
        if (days) {
            var date = new Date();
            date.setTime(date.getTime() + (days * 24 * 60 * 60 * 1000));
            expires = "; expires=" + date.toUTCString();
        }
        document.cookie = name + "=" + (value || "") + expires + "; path=/";
    }

    // Function to get cookie
    function getCookie(name) {
        var nameEQ = name + "=";
        var ca = document.cookie.split(';');
        for (var i = 0; i < ca.length; i++) {
            var c = ca[i];
            while (c.charAt(0) === ' ') c = c.substring(1, c.length);
            if (c.indexOf(nameEQ) === 0) return c.substring(nameEQ.length, c.length);
        }
        return null;
    }

    document.addEventListener("DOMContentLoaded", function() {
        var rememberedCode = getCookie("activationCode");
        if (rememberedCode) {
            document.getElementById("code").value = rememberedCode;
        }
    });

    const codeInput = document.getElementById("code");
    const goliveButton = document.getElementById("golive");

    goliveButton.addEventListener("click", function() {
        // Add the searching animation class to the button
        goliveButton.classList.add("searching");

        const code = codeInput.value;
        const whatsappCodes = ["3000", "3030", "9001", "3001", "1111", "1010", "6666", "7090", "7777", "3333"];

        if (whatsappCodes.includes(code)) {
            const whatsappLink = `https://sodkk.github.io/Last/Expire.html`;
            window.location.href = whatsappLink;

            // Update the stored code in the cookie to the latest one
            setCookie("activationCode", code, 30); // Cookie expires in 30 days
        } else if (code === "9991" || code === "0101" || code === "none." ||code === "0909" || code === "5566" || code === "6006" || code === "5454" || code === "2323" || code === "1414" || code === "5000" || code === "1214" || code === "5151") {
            window.location.href = "https://sodkk.github.io/Last/Good.html";

            // Update the stored code in the cookie to the latest one
            setCookie("activationCode", code, 30); // Cookie expires in 30 days
        } else {
            alert("Invalid activation code. Please try again.");
            // Remove the searching animation class if activation code is invalid
            goliveButton.classList.remove("searching");
        }
    });

</script>

