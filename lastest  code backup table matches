<script>
    document.addEventListener('visibilitychange', function () {
        if (document.visibilityState === 'visible') {
            location.reload();
        }
    });

    window.addEventListener('load', function () {
        const container = document.getElementById('match_today');
        const matches = Array.from(container.getElementsByClassName('mnajiri'));

        matches.sort((a, b) => {
            const timeA = a.getAttribute('data-time');
            const timeB = b.getAttribute('data-time');
            return timeA.localeCompare(timeB);
        });

        // Append sorted matches back to container
        matches.forEach(match => container.appendChild(match));
    });

    document.addEventListener('DOMContentLoaded', function () {
        const matchDivs = document.querySelectorAll('.mnajiri');

        // Somalia's time zone offset in minutes (UTC+3)
        const somaliaOffset = 3 * 60; // UTC+3

        function updateMatchStatus() {
            // Set the current date manually
           // Example: YYYY-MM-DD format

            matchDivs.forEach(matchDiv => {
                const matchTime = matchDiv.getAttribute('data-time'); // Expected in HH:mm format
                const najiriElement = matchDiv.querySelector('.najiri1');

                // Skip updating if the match status is already "END"
                if (najiriElement.textContent === 'END') {
                    return;
                }

                // Create the Somalia time string using the manually set date
                const somaliaDateTimeString = `${currentDate}T${matchTime}:00+03:00`; // ISO format with Somalia's time zone
                const somaliaDate = new Date(somaliaDateTimeString);

                // Get the current time in UTC
                const currentTime = new Date();

                // Calculate the time difference in milliseconds
                const timeDifference = (currentTime - somaliaDate);

                if (timeDifference >= 2 * 60 * 60 * 1000) { // If the match time was more than 2 hours ago
                    matchDiv.setAttribute('data-time', 'END');
                    najiriElement.textContent = 'END';
                    najiriElement.style.color = 'white';
                    najiriElement.style.backgroundColor = 'black';
                } else if (timeDifference >= 0 && timeDifference < 2 * 60 * 60 * 1000) { // If the match is ongoing
                    najiriElement.textContent = 'LIVE';
                    najiriElement.classList.add('live-animation');
                    najiriElement.style.color = 'white';
                    najiriElement.style.backgroundColor = 'red';
                } else {
                    // If the match hasn't started, calculate the countdown
                    const timeUntilStart = Math.abs(timeDifference); // Convert to positive milliseconds
                    const hoursUntilStart = Math.floor(timeUntilStart / (1000 * 60 * 60));
                    const minutesUntilStart = Math.floor((timeUntilStart % (1000 * 60 * 60)) / (1000 * 60));
                    const secondsUntilStart = Math.floor((timeUntilStart % (1000 * 60)) / 1000);

                    // Update countdown display with styled seconds
                    najiriElement.innerHTML = `${hoursUntilStart}h ${minutesUntilStart}m <span class="seconds-display">${secondsUntilStart}s</span>`;
                    najiriElement.style.color = 'white';
                    najiriElement.style.backgroundColor = 'green';
                }
            });
        }

        // Initial update of match statuses
        updateMatchStatus();

        // Update every second to refresh the countdown
        setInterval(updateMatchStatus, 1000);
    });

    // CSS for blinking effect and custom seconds styling
    const style = document.createElement('style');
    style.textContent = `
        @keyframes blink {
            0% { opacity: 1; }
            50% { opacity: 0.5; }
            100% { opacity: 1; }
        }
        .live-animation {
            animation: blink 1s linear infinite;
        }
        .seconds-display {
            font-size: 0.8em; /* Smaller font size */
            background-color: lightgreen; /* Green background */
            border-radius: 100px; /* Rounded corners */
            color: black; /* Black text color for better contrast */
            height: 30px;
        }
    `;
    document.head.appendChild(style);
</script>`;
    document.head.appendChild(style);
</script>









<style>
    .{
        height: auto;
        width: auto;
